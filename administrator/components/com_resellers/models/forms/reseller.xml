<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_RESELLERS_FORM_LBL_RESELLER_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_RESELLERS_FORM_LBL_RESELLER_CREATED_BY"
            description="COM_RESELLERS_FORM_DESC_RESELLER_CREATED_BY"  /> 

       <field name="name" type="text"
                
            label="COM_RESELLERS_FORM_LBL_RESELLER_NAME"
            description="COM_RESELLERS_FORM_DESC_RESELLER_NAME" 
                
                
                 required="true" 
            class="span6"
            filter="safehtml" /> 

       <field name="phone" type="text"
                
            label="COM_RESELLERS_FORM_LBL_RESELLER_PHONE"
            description="COM_RESELLERS_FORM_DESC_RESELLER_PHONE" 
                
                
                 required="true" 
            class="span6"
            filter="safehtml" /> 

 
		<field name="email" type="email" label="Email" description="" class="span6" validate="email"   /> 

       <field name="country" type="list"
            label="COM_RESELLERS_FORM_LBL_RESELLER_COUNTRY"
            description="COM_RESELLERS_FORM_DESC_RESELLER_COUNTRY" 
            required="true" 
            class="span6">
                <option value="Kenya">Kenya</option>
                <option value="Tanzania">Tanzania</option>
                <option value="Uganda">Uganda</option>
        </field> 

       <field name="longitude" type="text"
                
            label="COM_RESELLERS_FORM_LBL_RESELLER_LONGITUDE"
            description="COM_RESELLERS_FORM_DESC_RESELLER_LONGITUDE" 
                
                
                
            class="span6"
            filter="raw" /> 

       <field name="latitude" type="text"
                
            label="COM_RESELLERS_FORM_LBL_RESELLER_LATITUDE"
            description="COM_RESELLERS_FORM_DESC_RESELLER_LATITUDE" 
                
                
                
            class="span6"
            filter="safehtml" /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
